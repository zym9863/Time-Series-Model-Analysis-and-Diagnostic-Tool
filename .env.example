# 时间序列模型分析与诊断工具FastAPI服务配置

# 服务器配置
HOST=0.0.0.0
PORT=8000
WORKERS=4
LOG_LEVEL=info

# 开发模式配置
DEBUG=false
RELOAD=false

# CORS配置
CORS_ORIGINS=*
CORS_ALLOW_CREDENTIALS=true

# API配置
API_TITLE=时间序列模型分析与诊断API
API_VERSION=1.0.0
API_DESCRIPTION=提供AR模型平稳性检验和MA模型可逆性检验的REST API服务

# 限流配置（可选）
RATE_LIMIT_ENABLED=false
RATE_LIMIT_PER_MINUTE=60

# 监控配置（可选）
MONITORING_ENABLED=false
METRICS_PATH=/metrics

# 日志配置
LOG_FORMAT=json
LOG_FILE_PATH=logs/tsdiag-api.log
